# Discord Bot Token (lấy từ https://discord.com/developers/applications)
DISCORD_TOKEN=your_bot_token_here

# Bot Configuration
COMMAND_PREFIX=/
DEFAULT_LANGUAGE=vi

# Dictionary API Settings (Set to false to use local files only)
USE_DICTIONARY_API=true
API_TIMEOUT=5
ENABLE_WORD_CACHE=true
CACHE_SIZE=1000

# Game Settings
TURN_TIMEOUT=30
POINTS_CORRECT=1
POINTS_LONG_WORD=5
POINTS_RARE_WORD=10
POINTS_WRONG=-2
HINT_COST=10
PASS_COST=20
